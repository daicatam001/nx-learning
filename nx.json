{
  "npmScope": "nx-learning",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "angular-spotify": {
      "tags": []
    },
    "angular-spotify-e2e": {
      "tags": [],
      "implicitDependencies": ["angular-spotify"]
    },
    "web-layout": {
      "tags": []
    },
    "web-shared-data-access-models": {
      "tags": []
    },
    "web-shared-data-access-ui-store": {
      "tags": []
    },
    "web-shared-ui-nav-bar": {
      "tags": []
    },
    "web-shell": {
      "tags": []
    }
  }
}
